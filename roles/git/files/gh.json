{
  "version": "1.11.4",
  "api": {
    "host": "api.github.com",
    "protocol": "https"
  },
  "default_branch": "master",
  "default_remote": "origin",
  "default_pr_forwarder": "",
  "default_pr_reviewer": "",
  "github_token": "",
  "github_user": "p2kmgcl",
  "use_editor": true,
  "page_size": 30,
  "hooks": {
    "pull-request": {
      "close": {
        "before": [],
        "after": []
      },
      "fetch": {
        "before": [],
        "after": []
      },
      "fwd": {
        "before": [],
        "after": [
          "gh pr {{options.submittedPullNumber}} --user {{options.fwd}} --comment '/cc @{{options.submittedUser}}'",
          "gh pull-request {{options.number}} --user {{options.user}} --repo {{options.repo}} --comment 'Pull request forwarded to {{forwardedLink}}.{{#if options.changes}} [See changes here.]({{compareLink}}){{/if}}'",
          "gh pull-request {{options.number}} --close"
        ]
      },
      "open": {
        "before": [],
        "after": []
      },
      "submit": {
        "before": [],
        "after": [
          "{{#if options.submit == 'liferay-echo'}}gh pull-request {{options.submittedPull}} --user {{options.submit}} --comment 'ci:forward'{{/if}}"
        ]
      }
    }
  },
  "ignored_plugins": [],
  "pull_branch_name_prefix": "pr-",
  "plugins": {
    "jira": {
      "host": "issues.liferay.com",
      "user": "pablo.molina",
      "password": "",
      "base": "rest/api/2"
    }
  },
  "pretty_print": true,
  "replace": {},
  "ssh": true,
  "signature": "",
  "alias": {
    "daldi": "ealonso",
    "victor": "victorg1991",
    "sandro": "sandrodw3",
    "pavel": "pavel-savinov",
    "jurgen": "jkappler",
    "vero": "veroglez"
  }
}
