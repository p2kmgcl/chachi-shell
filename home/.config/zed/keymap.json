[
  {
    "context": "Workspace",
    "bindings": {
      "cmd-shift-g": "git_panel::ToggleFocus",
      "cmd-~": "terminal_panel::ToggleFocus",
      "cmd-shift-e": "project_panel::ToggleFocus",
    },
  },
  {
    "context": "right_dock == GitPanel",
    "bindings": { "cmd-shift-g": "workspace::ToggleRightDock" },
  },
  {
    "context": "bottom_dock == TerminalPanel",
    "bindings": { "cmd-~": "workspace::ToggleBottomDock" },
  },
  {
    "context": "left_dock == ProjectPanel",
    "bindings": { "cmd-shift-e": "workspace::ToggleLeftDock" },
  },
]
