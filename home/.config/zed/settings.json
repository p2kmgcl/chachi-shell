{
  "active_pane_modifiers": {
    "border_size": 0,
    "inactive_opacity": 0.5
  },
  "agent": {
    "default_profile": "ask",
    "always_allow_tool_actions": true
  },
  "auto_install_extensions": {
    "catppuccin": true,
    "dockerfile": true,
    "git-firefly": true,
    "github-classic": true,
    "html": true,
    "scss": true,
    "toml": true,
    "tsgo": true
  },
  "bottom_dock_layout": "full",
  "buffer_font_family": "Maple Mono NF CN",
  "buffer_font_size": 16,
  "buffer_line_height": "comfortable",
  "chat_panel": {
    "button": "when_in_call",
    "dock": "right"
  },
  "collaboration_panel": {
    "button": false
  },
  "current_line_highlight": "all",
  "diagnostics": {
    "button": true,
    "include_warnings": true,
    "inline": {
      "enabled": false,
      "max_severity": null,
      "min_column": 0,
      "padding": 4,
      "update_debounce_ms": 150
    },
    "lsp_pull_diagnostics": {
      "debounce_ms": 50,
      "enabled": true
    }
  },
  "disable_ai": true,
  "edit_predictions": {
    "mode": "subtle"
  },
  "expand_excerpt_lines": 0,
  "git": {
    "git_gutter": "tracked_files",
    "hunk_style": "unstaged_hollow",
    "inline_blame": {
      "enabled": false
    }
  },
  "git_hosting_providers": [],
  "git_panel": {
    "dock": "right"
  },
  "gutter": {
    "breakpoints": false,
    "folds": false,
    "line_numbers": true,
    "min_line_number_digits": 3,
    "runnables": false
  },
  "hide_mouse": "on_typing_and_movement",
  "icon_theme": "Zed (Default)",
  "indent_guides": {
    "coloring": "fixed"
  },
  "inlay_hints": {
    "enabled": false
  },
  "inline_code_actions": false,
  "languages": {
    "JavaScript": {
      "code_actions_on_format": { "source.fixAll.eslint": true }
    },
    "TypeScript": {
      "code_actions_on_format": { "source.fixAll.eslint": true }
    },
    "TSX": {
      "code_actions_on_format": { "source.fixAll.eslint": true }
    }
  },
  "lsp": {
    "eslint": {
      "settings": {
        "nodePath": ".yarn/sdks",
        "workingDirectory": {
          "mode": "auto"
        }
      }
    },
    "vtsls": {
      "settings": {
        "javascript": {
          "tsserver": { "maxTsServerMemory": 40000 }
        },
        "typescript": {
          "tsserver": { "maxTsServerMemory": 40000 }
        }
      }
    }
  },
  "max_tabs": 10,
  "message_editor": {
    "auto_replace_emoji_shortcode": true
  },
  "middle_click_paste": false,
  "minimap": {
    "current_line_highlight": "line",
    "display_in": "all_editors",
    "max_width_columns": 80,
    "show": "never",
    "thumb": "always",
    "thumb_border": "none"
  },
  "notification_panel": {
    "button": false
  },
  "outline_panel": {
    "button": false
  },
  "preferred_line_length": 80,
  "project_panel": {
    "auto_fold_dirs": true,
    "auto_reveal_entries": true,
    "button": true,
    "default_width": 500,
    "dock": "right",
    "entry_spacing": "standard",
    "file_icons": true,
    "folder_icons": true,
    "git_status": true,
    "hide_gitignore": true,
    "hide_root": true,
    "indent_guides": { "show": "always" },
    "indent_size": 24,
    "scrollbar": { "show": "auto" },
    "show_diagnostics": "all",
    "sticky_scroll": true
  },
  "relative_line_numbers": true,
  "scroll_beyond_last_line": "one_page",
  "scrollbar": {
    "cursors": false,
    "diagnostics": "all",
    "git_diff": true,
    "search_results": true,
    "selected_symbol": false,
    "selected_text": false,
    "show": "always"
  },
  "tab_bar": {
    "show": true,
    "show_nav_history_buttons": false,
    "show_tab_bar_buttons": false
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "button": true,
    "default_height": 400,
    "dock": "bottom",
    "font_family": "Maple Mono NF CN",
    "font_size": 16,
    "line_height": "standard",
    "toolbar": { "breadcrumbs": false }
  },
  "title_bar": {
    "show_branch_icon": true,
    "show_branch_name": true,
    "show_project_items": true
  },
  "theme": {
    "dark": "Catppuccin Mocha",
    "light": "Cattppuccin Latte",
    "mode": "system"
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "unnecessary_code_fade": 0.5,
  "ui_font_family": "Maple Mono NF CN",
  "ui_font_size": 16,
  "vertical_scroll_margin": 999,
  "vim_mode": true,
  "vim": {
    "highlight_on_yank_duration": 250,
    "toggle_relative_line_numbers": false,
    "use_system_clipboard": "always"
  }
}
